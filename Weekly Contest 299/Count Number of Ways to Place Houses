'''
https://leetcode.com/contest/weekly-contest-299/problems/count-number-of-ways-to-place-houses/
'''

class Solution:
    def countHousePlacements(self, n: int) -> int:
        x1=2
        x2=3
        if n==1:
            return x1*x1
        if n==2:
            return x2*x2
        else:
            while n>2:
                x3=x1+x2
                x1=x2
                x2=x3
                n-=1
            return x3*x3 % (10**9+7)
